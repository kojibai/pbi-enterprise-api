{
  "name": "presencebound-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the PresenceBound Enterprise API",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "presencebound",
    "pbi",
    "sdk"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "tsup": "^8.3.5",
    "typescript": "^5.5.4",
    "vitest": "^2.1.1"
  }
}
